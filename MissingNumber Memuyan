using System;
					
public class Program
{
	public static void Main()
	{
		//           0 1 2 3 4 5 6  
		int[] arr = {1,2,3,4,4,5,6,7,7,8,9,10,12,13}; // size = 7,  0-6
		Console.WriteLine(IndexOf(arr));
	}
	
	
	
	
	
	
	
	static int IndexOf(int[] a)
	{
		
		
		for(int i=1;i<a.Length;i++)
		{

				if (a[i] != a[i-1]+1 && a[i-1]!=a[i])	
					return a[i]-1;

		}
	
		
		return 11111;
	}
	
	
}
